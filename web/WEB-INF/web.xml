    </servlet>
    <servlet-mapping>
        <servlet-name>RegionView</servlet-name>
        <url-pattern>/RegionView</url-pattern>
    </servlet-mapping>
    <!-- Servlet Configuration for EmpWorldRequestController -->
    <servlet>
        <servlet-name>EmpRequest</servlet-name>
        <servlet-class>com.nhat.controller.EmpWorldController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmpRequest</servlet-name>
        <url-pattern>/EmpRequest</url-pattern>
    </servlet-mapping>
