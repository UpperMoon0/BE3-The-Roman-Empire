    <servlet>
        <servlet-name>ConsReportHistory</servlet-name>
        <servlet-class>com.nhat.controller.ConsReportHistoryController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConsReportHistory</servlet-name>
        <url-pattern>/ConsReportHistory</url-pattern>
    </servlet-mapping>
</web-app>
