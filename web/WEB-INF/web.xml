    <servlet>
        <servlet-name>EmpReport</servlet-name>
        <servlet-class>com.nhat.controller.EmpReportController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmpReport</servlet-name>
        <url-pattern>/EmpReport</url-pattern>
    </servlet-mapping>
    <!-- Servlet Configuration for EmpMenu -->
    <servlet>
        <servlet-name>EmpMenu</servlet-name>
        <servlet-class>com.nhat.controller.EmpMenuController</servlet-class>
    </servlet>
    <servlet-mapping>
